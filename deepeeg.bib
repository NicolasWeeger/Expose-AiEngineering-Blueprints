@Article{Sousa2024,
  author    = {de Sousa, Ana Maria Amaro and van Putten, Michel J.A.M. and van den Berg, Stéphanie and Amir Haeri, Maryam},
  journal   = {Biomedical Signal Processing and Control},
  title     = {Detection of Interictal epileptiform discharges with semi-supervised deep learning},
  year      = {2024},
  issn      = {1746-8094},
  month     = feb,
  pages     = {105610},
  volume    = {88},
  comment   = {Der Artikel beschreibt die Entwicklung und Evaluierung von unüberwachten und semi-überwachten Deep-Learning-Methoden zur automatisierten Detektion von interiktalen epileptiformen Entladungen (IEDs) in EEG-Aufzeichnungen. **Das Hauptziel ist die Verbesserung der Effizienz und Genauigkeit der Epilepsiediagnostik** , indem der zeitaufwendige und fehleranfällige visuelle Prozess durch klinische Experten unterstützt wird. Autoencoder (AE) und Variational Autoencoder (VAE) werden trainiert, um normale EEG-Muster zu lernen und Anomalien (IEDs) als Abweichungen von dieser Normalität zu identifizieren. Ein semi-überwachter Ansatz, der sowohl markierte als auch unmarkierte Daten nutzt, erzielt die **besten Ergebnisse mit einer Sensitivität von 81,9% und einer Spezifität von 91,7%** . Die Studie untersucht verschiedene Entscheidungsfindungskriterien und zeigt, dass ein **kombinierter Ansatz aus VAE und einem neuronalen Klassifikator die effektivste Methode** zur IED-Detektion darstellt.},
  doi       = {10.1016/j.bspc.2023.105610},
  file      = {:PDFs/Sousa2024.pdf:PDF},
  groups    = {new_added, IED Detection, Important_Literature},
  publisher = {Elsevier BV},
}

